{
  "name": "bare",
  "description": "A blank template to get started with Payload",
  "version": "1.0.0",
  "main": "dist/server.js",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env PAYLOAD_CONFIG_PATH=src/payload/payload.config.ts nodemon",
    "build:payload": "cross-env PAYLOAD_CONFIG_PATH=src/payload/payload.config.ts payload build",
    "build:server": "tsc --project tsconfig.server.json",
    "build:next": "cross-env PAYLOAD_CONFIG_PATH=dist/payload/payload.config.js NEXT_BUILD=true node dist/server.js",
    "build": "cross-env NODE_ENV=production pnpm build:payload && pnpm build:server && pnpm copyfiles && pnpm build:next",
    "serve": "cross-env PAYLOAD_CONFIG_PATH=dist/payload/payload.config.js NODE_ENV=production node dist/server.js",
    "copyfiles": "copyfiles -u 1 \"src/**/*.{html,css,scss,ttf,woff,woff2,eot,svg,jpg,png}\" dist/",
    "generate:types": "cross-env PAYLOAD_CONFIG_PATH=src/payload/payload.config.ts payload generate:types",
    "generate:graphQLSchema": "cross-env PAYLOAD_CONFIG_PATH=src/payload/payload.config.ts payload generate:graphQLSchema",
    "gen": "pnpm generate:types && pnpm generate:graphQLSchema",
    "lint": "eslint src",
    "lint:fix": "eslint --fix --ext .ts,.tsx src",
    "payload": "cross-env PAYLOAD_CONFIG_PATH=src/payload/payload.config.ts payload"
  },
  "dependencies": {
    "@payloadcms/bundler-webpack": "^1.0.0",
    "@payloadcms/db-postgres": "^0.x",
    "@payloadcms/plugin-cloud": "^2.0.0",
    "@payloadcms/plugin-nested-docs": "^1.0.10",
    "@payloadcms/plugin-redirects": "^1.0.1",
    "@payloadcms/plugin-seo": "^1.0.15",
    "@payloadcms/richtext-slate": "^1.0.0",
    "@types/escape-html": "^1.0.4",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "escape-html": "^1.0.3",
    "express": "^4.17.1",
    "next": "14.0.4",
    "payload": "^2.0.0",
    "payload-admin-bar": "^1.0.6",
    "qs": "^6.11.2",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.49.2",
    "slate": "^0.91.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.9",
    "@types/node": "^20",
    "@types/qs": "^6.9.11",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "copyfiles": "^2.4.1",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "nodemon": "^3.0.2",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "resolutions": {
    "jackspeak": "2.1.1"
  }
}
